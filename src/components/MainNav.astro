---
import AnalyticsBody from './AnalyticsBody.astro';
import { Image } from 'astro:assets'
import avatar from '../assets/avatar.png'
---
<style>
  .logo, .logo * {
      display: inline-block;
      vertical-align: middle;
   }
  .logo {
      margin: 1em 3em;
  }
  .logo img {
    margin-right: 1em;
  }
</style>

<AnalyticsBody />
<div class="logo">
    <a href="/">
        <Image width="60" height="60" src={avatar} alt="avatar" />
        <h1>JBeshir</h1>
    </a>
</div>